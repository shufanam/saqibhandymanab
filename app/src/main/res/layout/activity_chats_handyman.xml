                android:drawableEnd="@drawable/img_trash_gray_400"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/img_settings_white_a700"
                android:ellipsize="end"
                android:hint="@string/msg_write_a_message"
                android:id="@+id/etInput" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_22pxh"
                android:paddingStart="@dimen/_13pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@={chatsHandymanVM.chatsHandymanModel.etInputValue}"
